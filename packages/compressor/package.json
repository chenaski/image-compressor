{
  "name": "compressor",
  "private": true,
  "packageManager": "pnpm@7.13.2",
  "volta": {
    "extends": "../../package.json"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "dev": "nodemon src/index.ts",
    "format": "prettier -c .",
    "format:fix": "prettier -w .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier -w"
    ],
    "*.{json,md}": "prettier -w"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
